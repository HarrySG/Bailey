<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		<record model="ir.ui.view" id="debranding_partner_contact_form_view_inherit">
			<field name="name">debranding.partner.contact.form.view.inherit</field>
			<field name="model">res.partner</field>
			<field name="inherit_id" ref="base.view_partner_form" />
			<field name="priority" eval="99" />
			<field name="arch" type="xml">
				<xpath
					expr="//field[@name='website']"
					position="attributes">
					<attribute name="placeholder">e.g. www.commgate.net</attribute>
				</xpath>
			</field>
		</record>
    </data>
</openerp>
